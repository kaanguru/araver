{
  "manifest_version": 2,
  "name": "Ara Ver",
  "version": "0.1.0",

  "description": "Ara verme hatırlatıcısı,pausly (www.pausly.app) ",

  "background": {
    "scripts": ["options.js", "main.js"]
  },
  "permissions": ["idle", "alarms", "notifications", "storage"],
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "32": "icons/icons8_pause_32px.png",
    "64": "icons/icons8_pause_64px.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "icons/icons8_pause_32px.png",
      "64": "icons/icons8_pause_64px.png"
    },
    "default_title": "Ara Ver Ayarları",
    "default_popup": "options.html"
  }
}
